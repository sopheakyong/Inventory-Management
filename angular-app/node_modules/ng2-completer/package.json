    "shelljs": "^0.8.2",
    "source-map-loader": "^0.2.4",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "typescript": "~3.7.5",
    "uglify-js": "^3.1.6",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.10.2"
  },
